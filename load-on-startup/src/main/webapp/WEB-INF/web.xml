<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Apple</servlet-name>
		<servlet-class>com.ty.PageA</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Apple</servlet-name>
		<url-pattern>/first</url-pattern>
		
	</servlet-mapping>
	<servlet>
		<servlet-name>Bat</servlet-name>
		<servlet-class>com.ty.PageB</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bat</servlet-name>
		<url-pattern>/second</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Cat</servlet-name>
		<servlet-class>com.ty.PageC</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cat</servlet-name>
		<url-pattern>/third</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Dog</servlet-name>
		<servlet-class>com.ty.PageD</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Dog</servlet-name>
		<url-pattern>/fourth</url-pattern>
	</servlet-mapping>
</web-app>
