<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>Apple</servlet-name>
  <servlet-class>com.ty.Calculator</servlet-class>
  
  <init-param>
  <param-name>pi</param-name>
  <param-value>3.14</param-value>
  </init-param>
  <init-param>
  <param-name>rate</param-name>
  <param-value>5.2</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>Apple</servlet-name>
  <url-pattern>/first</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Ball</servlet-name>
  <servlet-class>com.ty.Bank</servlet-class>
  
  </servlet>
  <servlet-mapping>
  <servlet-name>Ball</servlet-name>
  <url-pattern>/second</url-pattern>
  </servlet-mapping>
  

</web-app>
